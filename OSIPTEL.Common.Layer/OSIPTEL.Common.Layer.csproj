  </PropertyGroup>

  <ItemGroup>
    <None Remove="oci.dll" />
    <None Remove="ocijdbc21.dll" />
    <None Remove="ociw32.dll" />
    <None Remove="oramysql.dll" />
    <None Remove="orannzsbb.dll" />
    <None Remove="oraocci21.dll" />
    <None Remove="oraocci21d.dll" />
